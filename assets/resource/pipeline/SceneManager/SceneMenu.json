{
    "ScenePrivateWorldEnterMenuRegionalDevelopment": {
        "doc": "从大世界进入地区建设菜单",
        "action": {
            "type": "ClickKey",
            "param": {
                "key": 89 // Y
            }
        },
        "next": [
            "[JumpBack]ScenePrivateWorldEnterMenuRegionalDevelopmentLoading",
