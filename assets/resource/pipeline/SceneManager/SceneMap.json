    "ScenePrivateAnyEnterMapLoading": {
        "doc": "等待切换到地图场景",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -350,
                    0,
                    350,
                    60
                ],
                "template": [
                    "SceneManager/MapMind.png"
                ]
            }
        },
        "inverse": true
    },
    "ScenePrivateMapEnterMapDijiang": {
        "doc": "在地图界面，进入帝江号地图",
        "recognition": {
            "type": "OCR",
            "param": {
                "roi": [
                    -260,
                    0,
                    260,
                    160
                ],
                "expected": [
                    "[Oo].?[Mm].?[Vv]"
                ]
            }
        },
        "action": {
            "type": "Click",
            "param": {
                "target_offset": [
                    0,
                    0,
                    0,
                    -5
                ]
            }
        },
        "next": [
            "[JumpBack]ScenePrivateAnyEnterMapLoading",
            "ScenePrivateMapEnterMapDijiangSuccess"
        ]
    },
    "ScenePrivateMapEnterMapDijiangSuccess": {
        "doc": "进入帝江号地图成功",
        "recognition": {
            "type": "OCR",
            "param": {
                "roi": [
                    0,
                    0,
                    350,
                    220
                ],
                "expected": [
                    "[Oo].?[Mm].?[Vv]"
                ]
            }
        }
    },
    "ScenePrivateMapEnterTeleport": {
        "doc": "在地图界面，随机选择一个传送点进入",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    0,
                    0,
                    0,
                    0
                ],
                "template": [
                    "SceneManager/MapTeleport.png"
                ]
            }
        },
        "action": {
            "type": "Click"
        },
        "post_delay": 800, // 等待弹框出现完毕
        "next": [
            "[JumpBack]ScenePrivateMapTeleportChoose",
            "ScenePrivateMapTeleportConfirm"
        ]
    },
    "ScenePrivateMapTeleportChoose": {
        "doc": "判断是否有多个图标重叠，导致出现候选条目",
        "recognition": {
            "type": "OCR",
            "param": {
                "roi": [
                    -500,
                    350,
                    400,
                    300
                ],
                "expected": [
                    "协议传送点",
                    "協議傳送點",
                    "TP Point",
                    "協約転送ポイント",
                    "프로토콜 전송 지점"
                ]
            }
        },
        "action": {
            "type": "Click"
        }
    },
    "ScenePrivateMapTeleportConfirm": {
        "doc": "点击确认传送",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -150,
                    -150,
                    150,
                    150
                ],
                "template": [
                    "SceneManager/MapTeleportConfirm.png"
                ]
            }
        },
        "action": {
            "type": "Click"
        },
        // 场景一：传送有loading
        // 场景二：原地传送无loading
        "next": [
            "[JumpBack]SceneWaitLoadingExit",
            "ScenePrivateMapTeleportSuccess"
        ]
    },
    "ScenePrivateMapTeleportSuccess": {
        "doc": "进入大世界，传送成功",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -200,
                    0,
                    200,
                    200
                ],
                "template": [
                    "SceneManager/WorldMenu.png"
                ],
                "green_mask": true
            }
        }
    }
}