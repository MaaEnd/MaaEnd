{
    "ScenePrivateAnyEnterWorld": {
        "doc": "无限循环esc，从任意界面进入谷地/武陵/帝江任意一个大世界",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -200,
                    0,
                    200,
                    200
                ],
                "template": [
                    "SceneManager/WorldMenu.png"
                ],
                "green_mask": true
            }
        },
        "inverse": true,
        "action": {
            "type": "ClickKey",
            "param": {
                "key": 27 // ESC
            }
        },
        "post_delay": 800, // 等待弹框出现完毕
        "next": [
            "[JumpBack]SceneDialogConfirm",
            "[JumpBack]ScenePrivateEndVisitConfirm",
            "[JumpBack]SceneWaitLoadingExit",
            "ScenePrivateNextTrue"
        ]
    },
    "ScenePrivateWorldFactoryDisableEnterFactoryActive": {
        "doc": "在大世界工厂模式不可用，例如战斗状态，进入工厂模式",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    0,
                    0,
                    60,
                    60
                ],
                "template": [
                    "SceneManager/WorldFactoryDisable.png"
                ]
            }
        },
        "next": [
            "[JumpBack]ScenePrivateWorldAnyEnterFactory",
            "ScenePrivateAnyEnterWorldFactorySuccess",
            "ScenePrivateWorldFactoryNotActiveEnterFactoryActive"
        ]
    },
    "ScenePrivateWorldFactoryNotActiveEnterFactoryActive": {
        "doc": "在大世界工厂模式未激活，进入工厂模式",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    0,
                    0,
                    60,
                    60
                ],
                "template": [
                    "SceneManager/WorldFactoryNotActive.png"
                ]
            }
        },
        "action": {
            "type": "ClickKey",
            "param": {
                "key": 9 // Tab
            }
        },
        "next": [
            "ScenePrivateAnyEnterWorldFactorySuccess"
        ]
    },
    "ScenePrivateWorldDijiangEnterFactoryActive": {
        "doc": "在大世界帝江号，进入工厂模式",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    0,
                    0,
                    400,
                    80
                ],
                "template": [
                    "SceneManager/WorldDijiangMenu.png"
                ],
                "green_mask": true
            }
        },
        "next": [
            "[JumpBack]ScenePrivateWorldAnyEnterFactory",
            "ScenePrivateAnyEnterWorldFactorySuccess",
            "ScenePrivateWorldFactoryNotActiveEnterFactoryActive"
        ]
    },
    "ScenePrivateWorldAnyEnterFactory": {
        "doc": "在任意大世界界面，进入工厂模式",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    0,
                    0,
                    60,
                    60
                ],
                "template": [
                    "SceneManager/WorldFactoryNotActive.png",
                    "SceneManager/WorldFactoryActive.png"
                ],
                "threshold": 0.9
            }
        },
        "inverse": true,
        "next": [
            "[JumpBack]ScenePrivateWorldEnterMapValleyIVTheHub",
            "ScenePrivateMapEnterTeleport"
        ]
    },
    "ScenePrivateAnyEnterWorldFactorySuccess": {
        "doc": "从任意界面进入大世界工厂模式成功",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    0,
                    0,
                    60,
                    60
                ],
                "template": [
                    "SceneManager/WorldFactoryActive.png"
                ]
            }
        }
    }
}