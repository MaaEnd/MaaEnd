    "Iat2MapClick": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "InterAreaTransporter/map.png",
                "roi": [
                    954,
                    492,
                    247,
                    175
                ],
                "threshold": 0.80
            }
        },
        "action": "Click",
        "post_delay": 500,
        "next": [
            "Iat2OMV",
            "Iat2Piont"
        ]
    },
    // 切换帝江号视图
