// 跨区域物资搬运，于帝江号协议传送点打开仓库后使用
// Iat(InterAreaTransporter) 
{
    // 入口
    "IatMain": {
        "next": "Iat2MapScoll"
    },
    // 滑动至地图露出
    "Iat2MapScoll": {
        "recognition": "DirectHit",
        "action": {
            "type": "Swipe",
            "param": {
                "begin": [
                    1100,
                    400
