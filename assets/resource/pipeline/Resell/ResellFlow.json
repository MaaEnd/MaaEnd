{
    "ResellTaskDelay": {
        "doc": "可被 pipeline_override 覆盖延迟时间的占位节点",
        "recognition": "DirectHit",
        "action": "DoNothing",
        "next": []
    },
    "ResellCheckQuota": {
        "doc": "检查配额溢出状态",
        "recognition": "DirectHit",
        "pre_wait_freezes": 300,
        "action": "Custom",
        "custom_action": "ResellCheckQuotaAction",
        "next": [
            "ResellScan"
        ]
    },
    "ResellDecide": {
        "doc": "根据利润和配额决策购买或切换地区",
        "recognition": "DirectHit",
        "action": "Custom",
        "custom_action": "ResellDecideAction",
        "next": []
    },
    "ResellScan": {
        "doc": "扫描指定行列的商品，custom_action_param 传入 {row, col}",
        "recognition": "DirectHit",
        "action": "Custom",
        "custom_action": "ResellScanProductAction",
        "custom_action_param": {
            "row": 1,
            "col": 1
        },
        "next": []
    }
}
