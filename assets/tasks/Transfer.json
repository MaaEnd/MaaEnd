{
    "task": [
        {
            "name": "Transfer",
            "label": "$task.Transfer.label",
            "entry": "Transfer",
            "default_check": false,
            "description": "$task.Transfer.description",
            "option": [
                "TransferDirection"
            ]
        }
    ],
    "option": {
        "TransferDirection": {
            "type": "select",
            "label": "$option.TransferDirection.label",
            "cases": [
                {
                    "name": "4To50",
                    "label": "$option.TransferDirection.cases.4To50.label",
                    "pipeline_override": {
                        "TransferStart": {
                            "next": [
                                "SwitchRepoToNo4"
                            ]
                        }
                    }
                },
                {
                    "name": "50To4",
                    "label": "$option.TransferDirection.cases.50To4.label",
                    "pipeline_override": {
                        "TransferStart": {
                            "next": [
                                "SwitchRepoTo50"
                            ]
                        },
                        "CtrlKeyUp": {
                            "next": [
                                "SwitchRepoToNo4"
                            ]
                        },
                        "ClickEscNo4": {
                            "next": [
                                "BagToRepo"
                            ]
                        },
                        "ClickEsc50": {
                            "next": [
                                "RepoToBag"
                            ]
                        }
                    }
                }
            ]
        }
    }
}
