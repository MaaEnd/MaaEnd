{
    "task": [
        {
            "name": "ItemTransfer",
            "label": "$task.ItemTransfer.label",
            "entry": "ItemTransfer",
            "default_check": true,
            "description": "$task.ItemTransfer.description",
            "option": [
                "WhatToTransfer",
                "TransferAll",
                "OriginRegion",
                "DestinationRegion"
            ],
            "controller": [
                "Win32",
                "Win32-Window",
                "Win32-Front"
            ],
            "pipeline_override": {
                "ClickOrigin": {
                    "expected": "四号谷地"
                },
                "ClickDestination": {
                    "expected": "武陵"
                }
            }
        }
    ],
    "option": {
        "WhatToTransfer": {
            "type": "select",
            "label": "$option.ItemTransferWhatToTransfer.label",
            "description": "$option.ItemTransferWhatToTransfer.description",
            "cases": [
                {
                    "name": "蓝铁矿",
                    "label": "$蓝铁矿",
                    "pipeline_override": {
                        "FindItem": {
                            "template": "ItemTransfer/蓝铁矿.png"
                        }
                    }
                },
                {
                    "name": "高容谷地电池",
                    "label": "$高容谷地电池",
                    "pipeline_override": {
                        "FindItem": {
                            "template": "ItemTransfer/高容谷地电池.png"
                        }
                    }
                },
                {
                    "name": "源矿",
                    "label": "$源矿",
                    "pipeline_override": {
                        "FindItem": {
                            "template": "ItemTransfer/源矿.png"
                        }
                    }
                },
                {
                    "name": "砂叶",
                    "label": "$砂叶",
                    "pipeline_override": {
                        "FindItem": {
                            "template": "ItemTransfer/砂叶.png"
                        }
                    }
                }
            ],
            "default_case": "蓝铁矿"
        },
        "TransferAll": {
            "type": "switch",
            "label": "$option.ItemTransferTransferAll.label",
            "description": "$option.ItemTransferTransferAll.description",
            "cases": [
                {
                    "name": "Yes"
                },
                {
                    "name": "No",
                    "option": [
                        "TransferTimes"
                    ]
                }
            ],
            "default_case": "Yes"
        },
        "TransferTimes": {
            "type": "input",
            "label": "$option.ItemTransferTransferTimes.label",
            "inputs": [
                {
                    "name": "Times",
                    "label": "$option.ItemTransferTransferTimes.input.label",
                    "description": "$option.ItemTransferTransferTimes.input.description",
                    "default": "1",
                    "pipeline_type": "int",
                    "verify": "^((?!0)\\d+|0)$",
                    "pattern_msg": "$option.ItemTransferTransferTimes.input.error"
                }
            ],
            "pipeline_override": {
                "ItemTransferStart": {
                    "max_hit": "{Times}"
                }
            }
        },
        "OriginRegion": {
            "type": "select",
            "label": "$option.ItemTransferOriginRegion.label",
            "description": "$option.ItemTransferOriginRegion.description",
            "cases": [
                {
                    "name": "ValleyIV",
                    "label": "$global.region.ValleyIV",
                    "pipeline_override": {
                        "ClickOrigin": {
                            "expected": "四号谷地"
                        }
                    }
                },
                {
                    "name": "Wuling",
                    "label": "$global.region.Wuling",
                    "pipeline_override": {
                        "ClickOrigin": {
                            "expected": "武陵"
                        }
                    }
                }
            ],
            "default_case": "ValleyIV"
        },
        "DestinationRegion": {
            "type": "select",
            "label": "$option.ItemTransferDestinationRegion.label",
            "description": "$option.ItemTransferDestinationRegion.description",
            "cases": [
                {
                    "name": "ValleyIV",
                    "label": "$global.region.ValleyIV",
                    "pipeline_override": {
                        "ClickDestination": {
                            "expected": "四号谷地"
                        }
                    }
                },
                {
                    "name": "Wuling",
                    "label": "$global.region.Wuling",
                    "pipeline_override": {
                        "ClickDestination": {
                            "expected": "武陵"
                        }
                    }
                }
            ],
            "default_case": "Wuling"
        }
    }
}