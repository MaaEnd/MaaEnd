{
    "task": [
        {
            "name": "DijiangRewards",
            "label": "$task.DijiangRewards.label",
            "entry": "DijiangRewards",
            "description": "$task.DijiangRewards.description",
            "option": [
                "AutoStartExchange",
                "ProductionAssistanceWithoutTheControlNexus",
                "ClueSetting",
                "SelectToGrow"
            ]
        }
    ],
    "option": {
        "AutoStartExchange": {
            "type": "switch",
            "label": "$option.AutoStartExchange.label",
            "description": "$option.AutoStartExchange.description",
            "default_case": "No",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "StartExchange": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "StartExchange": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "ProductionAssistanceWithoutTheControlNexus": {
            "type": "switch",
            "label": "$option.ProductionAssistanceWithoutTheControlNexus.label",
            "description": "$option.ProductionAssistanceWithoutTheControlNexus.description",
            "default_case": "No",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "RecoveryEmotion": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "RecoveryEmotion": {
                            "enabled": true
                        }
                    }
                }
            ]
        },
        "ClueSetting": {
            "type": "switch",
            "label": "$option.ClueSetting.label",
            "description": "$option.ClueSetting.description",
            "default_case": "No",
            "cases": [
                {
                    "name": "Yes",
                    "option": [
                        "ClueSend",
                        "ClueStockLimit"
                    ]
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "SendTheMostClues": {
                            "max_hit": 3
                        },
                        "ClueItemCount": {
                            "expected": "^(?:[4-9]|[1-9]\\d+)$"
                        }
                    }
                }
            ]
        },
        "ClueSend": {
            "type": "input",
            "label": "$option.ClueSend.label",
            "inputs": [
                {
                    "name": "MaxClueSend",
                    "label": "$option.ClueSend.MaxClueSend.label",
                    "pipeline_type": "int",
                    "default": "3"
                }
            ],
            "pipeline_override": {
                "SendTheMostClues": {
                    "max_hit": "{MaxClueSend}"
                }
            }
        },
        "ClueStockLimit": {
            "type": "select",
            "label": "$option.ClueStockLimit.label",
            "description": "$option.ClueStockLimit.description",
            "default_case": "3",
            "cases": [
                {
                    "name": "1",
                    "label": "$option.ClueStockLimit.cases.1.label",
                    "pipeline_override": {
                        "ClueItemCount": {
                            "expected": "^(?:[2-9]|[1-9]\\d+)$"
                        }
                    }
                },
                {
                    "name": "2",
                    "label": "$option.ClueStockLimit.cases.2.label",
                    "pipeline_override": {
                        "ClueItemCount": {
                            "expected": "^(?:[3-9]|[1-9]\\d+)$"
                        }
                    }
                },
                {
                    "name": "3",
                    "label": "$option.ClueStockLimit.cases.3.label",
                    "pipeline_override": {
                        "ClueItemCount": {
                            "expected": "^(?:[4-9]|[1-9]\\d+)$"
                        }
                    }
                },
                {
                    "name": "4",
                    "label": "$option.ClueStockLimit.cases.4.label",
                    "pipeline_override": {
                        "ClueItemCount": {
                            "expected": "^(?:[5-9]|[1-9]\\d+)$"
                        }
                    }
                }
            ]
        },
        "SelectToGrow": {
            "type": "select",
            "label": "$option.SelectToGrow.label",
            "description": "$option.SelectToGrow.description",
            "default_case": "Any",
            "cases": [
                {
                    "name": "DoNothing",
                    "label": "$option.SelectToGrow.cases.DoNothing.label",
                    "pipeline_override": {
                        "Grow": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Any",
                    "label": "$option.SelectToGrow.cases.Any.label",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "武陵石",
                                "燎石",
                                "岩天使叶",
                                "受蚀玉化叶",
                                "星门菌",
                                "血菌",
                                "重黯石",
                                "中黯石",
                                "轻黯石",
                                "至晶多齿叶",
                                "纯晶多齿叶",
                                "晶化多齿叶",
                                "重红柱状菌",
                                "中红柱状菌",
                                "轻红柱状菌",
                                "Wulingstone",
                                "Igneosite",
                                "False Aggela",
                                "Blighted Jadeleaf",
                                "Cosmagaric",
                                "Bloodcap",
                                "Umbronyx",
                                "Auronyx",
                                "Kalkonyx",
                                "Vitrodendra",
                                "Chrysodendra",
                                "Kalkodendra",
                                "Ruby Bolete",
                                "Red Bolete",
                                "Pink Bolete",
                                "岩アンゲロス葉",
                                "侵蝕玉化葉",
                                "星門茸",
                                "血潮茸",
                                "重闇石",
                                "中闇石",
                                "軽闇石",
                                "至晶鋭葉",
                                "純晶鋭葉",
                                "晶華鋭葉",
                                "深紅茸",
                                "紅色茸",
                                "薄紅茸",
                                "岩天使葉",
                                "受蝕玉化葉",
                                "星門菌",
                                "輕黯石",
                                "至晶多齒葉",
                                "純晶多齒葉",
                                "晶華多齒葉",
                                "重紅柱狀菌",
                                "中紅柱狀菌",
                                "輕紅柱狀菌"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedAny",
                        "SortBy",
                        "SortOrder"
                    ]
                },
                {
                    "name": "Wulingstone",
                    "label": "$item.Wulingstone",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "武陵石",
                                "Wulingstone"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Igneosite",
                    "label": "$item.Igneosite",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "燎石",
                                "Igneosite"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "FalseAggela",
                    "label": "$item.FalseAggela",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "岩天使叶",
                                "False Aggela",
                                "岩アンゲロス葉",
                                "岩天使葉"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "BlightedJadeleaf",
                    "label": "$item.BlightedJadeleaf",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "受蚀玉化叶",
                                "Blighted Jadeleaf",
                                "侵蝕玉化葉",
                                "受蝕玉化葉"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Cosmagaric",
                    "label": "$item.Cosmagaric",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "星门菌",
                                "Cosmagaric",
                                "星門茸",
                                "星門菌"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Bloodcap",
                    "label": "$item.Bloodcap",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "血菌",
                                "Bloodcap",
                                "血潮茸"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Umbronyx",
                    "label": "$item.Umbronyx",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "重黯石",
                                "Umbronyx",
                                "重闇石"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Auronyx",
                    "label": "$item.Auronyx",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "中黯石",
                                "Auronyx",
                                "中闇石"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Kalkonyx",
                    "label": "$item.Kalkonyx",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "轻黯石",
                                "Kalkonyx",
                                "軽闇石",
                                "輕黯石"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Vitrodendra",
                    "label": "$item.Vitrodendra",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "至晶多齿叶",
                                "Vitrodendra",
                                "至晶鋭葉",
                                "至晶多齒葉"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Chrysodendra",
                    "label": "$item.Chrysodendra",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "纯晶多齿叶",
                                "Chrysodendra",
                                "純晶鋭葉",
                                "純晶多齒葉"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "Kalkodendra",
                    "label": "$item.Kalkodendra",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "晶化多齿叶",
                                "Kalkodendra",
                                "晶華鋭葉",
                                "晶華多齒葉"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "RubyBolete",
                    "label": "$item.RubyBolete",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "重红柱状菌",
                                "Ruby Bolete",
                                "深紅茸",
                                "重紅柱狀菌"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "RedBolete",
                    "label": "$item.RedBolete",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "中红柱状菌",
                                "Red Bolete",
                                "紅色茸",
                                "中紅柱狀菌"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                },
                {
                    "name": "PinkBolete",
                    "label": "$item.PinkBolete",
                    "pipeline_override": {
                        "SelectGrowTarget": {
                            "expected": [
                                "轻红柱状菌",
                                "Pink Bolete",
                                "薄紅茸",
                                "輕紅柱狀菌"
                            ]
                        }
                    },
                    "option": [
                        "AutoExtractSeedOther"
                    ]
                }
            ]
        },
        "AutoExtractSeedAny": { // 适用于选择作物任务中任意选项的是否自动提取基核的选项
            "type": "switch",
            "label": "$option.AutoExtrachSeed.label",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "ExtractSeed": {
                            "next": [
                                "GrayCancelButton",
                                "NoMaterials"
                            ]
                        },
                        "NoMaterials": {
                            "next": []
                        }
                    }
                }
            ]
        },
        "AutoExtractSeedOther": { // 适用于除选择作物任务以外的其他任务中是否自动提取基核的选项
            "type": "switch",
            "label": "$option.AutoExtrachSeed.label",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "ExtractSeed": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "SortBy": {
            "type": "select",
            "label": "$option.SortBy.label",
            "default_case": "Default",
            "cases": [
                {
                    "name": "Default",
                    "label": "$option.SortBy.cases.Default.label",
                    "pipeline_override": {
                        "SortBy": {
                            "expected": [
                                "拥有数量",
                                "基核数量",
                                "品质",
                                "持有數量",
                                "基核數量",
                                "品質",
                                "総在庫",
                                "基核数",
                                "品質",
                                "Amount Owned",
                                "Amount of Seeds",
                                "Quality"
                            ]
                        },
                        "ChooseSortBy": {
                            "expected": [
                                "默认",
                                "預設",
                                "Default",
                                "デフォルト"
                            ]
                        },
                        "SwipeChooseSortBy": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "AmountOwned",
                    "label": "$option.SortBy.cases.AmountOwned.label",
                    "pipeline_override": {
                        "SortBy": {
                            "expected": [
                                "默认",
                                "基核数量",
                                "品质",
                                "預設",
                                "基核數量",
                                "品質",
                                "デフォルト",
                                "基核数",
                                "品質",
                                "Default",
                                "Amount of Seeds",
                                "Quality"
                            ]
                        },
                        "ChooseSortBy": {
                            "expected": [
                                "拥有数量",
                                "持有數量",
                                "総在庫",
                                "Amount Owned"
                            ]
                        }
                    }
                },
                {
                    "name": "AmountOfSeeds",
                    "label": "$option.SortBy.cases.AmountOfSeeds.label",
                    "pipeline_override": {
                        "SortBy": {
                            "expected": [
                                "默认",
                                "拥有数量",
                                "品质",
                                "預設",
                                "持有數量",
                                "品質",
                                "デフォルト",
                                "総在庫",
                                "品質",
                                "Default",
                                "Amount Owned",
                                "Quality"
                            ]
                        },
                        "ChooseSortBy": {
                            "expected": [
                                "基核数量",
                                "基核數量",
                                "基核数",
                                "Amount of Seeds"
                            ]
                        }
                    }
                },
                {
                    "name": "Quality",
                    "label": "$option.SortBy.cases.Quality.label",
                    "pipeline_override": {
                        "SortBy": {
                            "expected": [
                                "默认",
                                "拥有数量",
                                "基核数量",
                                "預設",
                                "持有數量",
                                "基核數量",
                                "デフォルト",
                                "総在庫",
                                "基核数",
                                "Default",
                                "Amount Owned",
                                "Amount of Seeds"
                            ]
                        },
                        "ChooseSortBy": {
                            "expected": [
                                "品质",
                                "品質",
                                "品質",
                                "Quality"
                            ]
                        }
                    }
                }
            ]
        },
        "SortOrder": {
            "type": "select",
            "label": "$option.SortOrder.label",
            "default_case": "ASC",
            "cases": [
                {
                    "name": "ASC",
                    "label": "$option.SortOrder.cases.Ascending.label",
                    "pipeline_override": {
                        "SortOrder": {
                            "enabled": true,
                            "expected": [
                                "降序",
                                "降序",
                                "DESC",
                                "降順"
                            ]
                        }
                    }
                },
                {
                    "name": "DESC",
                    "label": "$option.SortOrder.cases.Descending.label",
                    "pipeline_override": {
                        "SortOrder": {
                            "enabled": true,
                            "expected": [
                                "升序",
                                "升序",
                                "ASC",
                                "昇順"
                            ]
                        }
                    }
                }
            ]
        }
    }
}
