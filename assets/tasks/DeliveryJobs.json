{
    "task": [
        {
            "name": "DeliveryJobs",
            "label": "$task.DeliveryJobs.label",
            "entry": "DeliveryJobsMain",
            "description": "$task.DeliveryJobs.description",
            "option": [
                "ValleyIV",
                "Wuling"
            ]
        }
    ],
    "option": {
        "ValleyIV": {
            "type": "switch",
            "label": "$global.region.ValleyIV",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "DeliveryJobsValleyIV": {
                            "enabled": true
                        }
                    },
                    "option": [
                        "OriginiumSciencePark",
                        "OriginLodespring",
                        "PowerPlateau"
                    ]
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "DeliveryJobsValleyIV": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "OriginiumSciencePark": {
            "type": "switch",
            "label": "源石研究园",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "EnterOriginiumScienceParkCargo": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "EnterOriginiumScienceParkCargo": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "OriginLodespring": {
            "type": "switch",
            "label": "矿脉源区",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "EnterOriginLodespringCargo": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "EnterOriginLodespringCargo": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "PowerPlateau": {
            "type": "switch",
            "label": "供能高地",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "EnterPowerPlateauCargo": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "EnterPowerPlateauCargo": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "Wuling": {
            "type": "switch",
            "label": "$global.region.Wuling",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "DeliveryJobsWuling": {
                            "enabled": true
                        }
                    },
                    "option": [
                        "WulingCity"
                    ]
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "DeliveryJobsWuling": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "WulingCity": {
            "type": "switch",
            "label": "武陵城",
            "default_case": "Yes",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "EnterWulingCityCargo": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "EnterWulingCityCargo": {
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}
