{
    "task": [
        {
            "name": "CreditShopping",
            "label": "$task.CreditShopping.label",
            "entry": "CreditShoppingMain",
            "description": "$task.CreditShopping.description",
            "option": [
                "CreditShoppingOptions",
                "CreditShoppingForce",
                "CreditShoppingOnlyDiscount",
                "CreditShoppingReserve"
            ]
        }
    ],
    "option": {
        "CreditShoppingOptions": {
            "type": "input",
            "label": "$option.CreditShoppingOptions.label",
            "inputs": [
                {
                    "name": "buy_first",
                    "label": "$option.CreditShoppingOptions.inputs.buy_first.label",
                    "description": "$option.CreditShoppingOptions.inputs.buy_first.description",
                    "pipeline_type": "string",
                    "default": "嵌晶玉|武库配额"
                },
                {
                    "name": "blacklist",
                    "label": "$option.CreditShoppingOptions.inputs.blacklist.label",
                    "description": "$option.CreditShoppingOptions.inputs.blacklist.description",
                    "pipeline_type": "string",
                    "default": ""
                }
            ],
            "pipeline_override": {
                "CreditShoppingBuyFirst": {
                    "focus": {
                        "Node.Recognition.Succeeded": "覆盖{buy_first}"
                    },
                    "all_of": [
                        {
                            "sub_name": "CreditIcon",
                            "recognition": "TemplateMatch",
                            "template": "CreditShopping/CreditIcon.png",
                            "roi": [
                                74,
                                103,
                                1131,
                                538
                            ],
                            "order_by": "vertical"
                        },
                        {
                            "doc": "非售罄",
                            "sub_name": "NotSoldOut",
                            "recognition": "ColorMatch",
                            "method": 6,
                            "roi": "CreditIcon",
                            "roi_offset": [
                                -95,
                                -145,
                                150,
                                200
                            ],
                            "lower": [
                                120
                            ],
                            "upper": [
                                255
                            ],
                            "count": 20000
                        },
                        {
                            "doc": "按用户指定顺序购买",
                            "sub_name": "BuyFirstOCR",
                            "recognition": "OCR",
                            "roi": "NotSoldOut",
                            "roi_offset": [
                                0,
                                170,
                                0,
                                -160
                            ],
                            "expected": "^({buy_first})$",
                            "order_by": "Expected"
                        },
                        {
                            "doc": "买得起（价格不是红色）",
                            "sub_name": "Affordable",
                            "recognition": "ColorMatch",
                            "roi": "BuyFirstOCR",
                            "roi_offset": [
                                65,
                                -40,
                                -20,
                                1
                            ],
                            "lower": [
                                76,
                                76,
                                76
                            ],
                            "upper": [
                                108,
                                108,
                                108
                            ],
                            "count": 20,
                            "order_by": "vertical"
                        }
                    ]
                },
                "CreditShoppingBuyNormalNoDiscount": {
                    "focus": {
                        "Node.Recognition.Succeeded": "覆盖{Blacklist}无折扣"
                    },
                    "all_of": [
                        {
                            "sub_name": "CreditIcon",
                            "recognition": "TemplateMatch",
                            "template": "CreditShopping/CreditIcon.png",
                            "roi": [
                                74,
                                103,
                                1131,
                                538
                            ],
                            "order_by": "vertical"
                        },
                        {
                            "doc": "非售罄",
                            "sub_name": "NotSoldOut",
                            "recognition": "ColorMatch",
                            "method": 6,
                            "roi": "CreditIcon",
                            "roi_offset": [
                                -95,
                                -133,
                                130,
                                160
                            ],
                            "lower": [
                                120
                            ],
                            "upper": [
                                255
                            ],
                            "count": 20000
                        },
                        {
                            "sub_name": "BlacklistOCR",
                            "recognition": "OCR",
                            "roi": "NotSoldOut",
                            "roi_offset": [
                                0,
                                170,
                                0,
                                -160
                            ],
                            "order_by": "vertical",
                            "expected": "^(?!({Blacklist})$).*$"
                        },
                        {
                            "doc": "买得起（价格不是红色）",
                            "sub_name": "Affordable",
                            "recognition": "ColorMatch",
                            "roi": "BlacklistOCR",
                            "roi_offset": [
                                65,
                                -40,
                                -20,
                                1
                            ],
                            "lower": [
                                76,
                                76,
                                76
                            ],
                            "upper": [
                                108,
                                108,
                                108
                            ],
                            "count": 20,
                            "order_by": "vertical"
                        }
                    ]
                },
                "CreditShoppingBuyNormalWithDiscount": {
                    "focus": {
                        "Node.Recognition.Succeeded": "覆盖{Blacklist}折扣"
                    },
                    "all_of": [
                        {
                            "sub_name": "CreditIcon",
                            "recognition": "TemplateMatch",
                            "template": "CreditShopping/CreditIcon.png",
                            "roi": [
                                74,
                                103,
                                1131,
                                538
                            ],
                            "order_by": "vertical"
                        },
                        {
                            "doc": "非售罄",
                            "sub_name": "NotSoldOut",
                            "recognition": "ColorMatch",
                            "method": 6,
                            "roi": "CreditIcon",
                            "roi_offset": [
                                -95,
                                -133,
                                130,
                                160
                            ],
                            "lower": [
                                120
                            ],
                            "upper": [
                                255
                            ],
                            "count": 20000
                        },
                        {
                            "sub_name": "BlacklistOCR",
                            "recognition": "OCR",
                            "roi": "NotSoldOut",
                            "roi_offset": [
                                0,
                                170,
                                0,
                                -160
                            ],
                            "order_by": "vertical",
                            "expected": "^(?!({Blacklist})$).*$"
                        },
                        {
                            "doc": "买得起（价格不是红色）",
                            "sub_name": "Affordable",
                            "recognition": "ColorMatch",
                            "roi": "BlacklistOCR",
                            "roi_offset": [
                                65,
                                -40,
                                -20,
                                1
                            ],
                            "lower": [
                                76,
                                76,
                                76
                            ],
                            "upper": [
                                108,
                                108,
                                108
                            ],
                            "count": 20,
                            "order_by": "vertical"
                        },
                        {
                            "doc": "有折扣（价格是黄色）",
                            "sub_name": "IsDiscount",
                            "recognition": "ColorMatch",
                            "roi": "NotSoldOut",
                            "roi_offset": [
                                100,
                                0,
                                -100,
                                -150
                            ],
                            "lower": [
                                255,
                                220,
                                0
                            ],
                            "upper": [
                                255,
                                220,
                                0
                            ],
                            "count": 500,
                            "order_by": "vertical"
                        }
                    ]
                }
            }
        },
        "CreditShoppingForce": {
            "type": "switch",
            "label": "$option.CreditShoppingForce.label",
            "default_case": "No",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "CreditShoppingBuyBlacklist": {
                            "focus": {
                                "Node.Recognition.Succeeded": "开启溢出时无视黑名单"
                            },
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "CreditShoppingBuyBlacklist": {
                            "focus": {
                                "Node.Recognition.Succeeded": "关闭溢出时无视黑名单"
                            },
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "CreditShoppingOnlyDiscount": {
            "type": "switch",
            "label": "$option.CreditShoppingOnlyDiscount.label",
            "description": "$option.CreditShoppingOnlyDiscount.description",
            "default_case": "No",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "CreditShoppingBuyNormalNoDiscount": {
                            "enabled": false
                        },
                        "CreditShoppingBuyNormalWithDiscount": {
                            "focus": {
                                "Node.Recognition.Succeeded": "开启只购买打折物品"
                            },
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "CreditShoppingBuyNormalNoDiscount": {
                            "focus": {
                                "Node.Recognition.Succeeded": "关闭只购买打折物品"
                            },
                            "enabled": true
                        },
                        "CreditShoppingBuyNormalWithDiscount": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "CreditShoppingReserve": {
            "type": "switch",
            "label": "$option.CreditShoppingReserve.label",
            "description": "$option.CreditShoppingReserve.description",
            "default_case": "No",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "CreditShoppingReserveCredit": {
                            "focus": {
                                "Node.Recognition.Succeeded": "开启信用点低于300时停止"
                            },
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "CreditShoppingReserveCredit": {
                            "focus": {
                                "Node.Recognition.Succeeded": "关闭信用点低于300时停止"
                            },
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}