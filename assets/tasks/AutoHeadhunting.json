{
    "task": [
        {
            "name": "AutoHeadhunting",
            "label": "$task.AutoHeadhunting.label",
            "entry": "AutoHeadhunting",
            "description": "$task.AutoHeadhunting.description",
            "controller": [
                "Win32",
                "Win32-Window",
                "Win32-Front"
            ],
            "option": [
                "TargetPulls",
                "TargetOperator",
                "TargetOperatorNum",
                "PreferMode"
            ]
        }
    ],
    "option": {
        "TargetPulls": {
            "type": "input",
            "label": "$option.TargetPulls.label",
            "inputs": [
                {
                    "name": "TargetPulls",
                    "label": "$option.TargetPulls.label",
                    "description": "$option.TargetPulls.inputs.TargetPulls.description",
                    "pipeline_type": "int",
                    "verify": "^\\d+$",
                    "default": "0"
                }
            ],
            "pipeline_override": {
                "AutoHeadhunting": {
                    "attach": {
                        "TargetPulls": "{TargetPulls}"
                    }
                }
            }
        },
        "TargetOperatorNum": {
            "type": "input",
            "label": "$option.TargetOperatorNum.label",
            "inputs": [
                {
                    "name": "TargetOperatorNum",
                    "label": "$option.TargetOperatorNum.label",
                    "description": "$option.TargetOperatorNum.inputs.TargetOperatorNum.description",
                    "pipeline_type": "int",
                    "verify": "^\\d+$",
                    "default": "1"
                }
            ],
            "pipeline_override": {
                "AutoHeadhunting": {
                    "attach": {
                        "TargetOperatorNum": "{TargetOperatorNum}"
                    }
                }
            }
        },
        "PreferMode": {
            "type": "select",
            "label": "$option.PreferMode.label",
            "default_case": "1x",
            "cases": [
                {
                    "name": "1x",
                    "label": "$option.PreferMode.cases.1x.label",
                    "pipeline_override": {
                        "AutoHeadhunting": {
                            "attach": {
                                "PreferMode": 1
                            }
                        }
                    }
                },
                {
                    "name": "10x",
                    "label": "$option.PreferMode.cases.10x.label",
                    "pipeline_override": {
                        "AutoHeadhunting": {
                            "attach": {
                                "PreferMode": 10
                            }
                        }
                    }
                }
            ]
        },
        "TargetOperator": {
            "type": "select",
            "label": "$option.TargetOperator.label",
            "description": "$option.TargetOperator.inputs.TargetOperator.description",
            "default_case": "6stars",
            "cases": [
                {
                    "name": "None",
                    "label": "$option.TargetOperator.cases.None.label",
                    "pipeline_override": {
                        "AutoHeadhunting": {
                            "attach": {
                                "TargetOperator": ""
                            }
                        }
                    }
                },
                {
                    "name": "6stars",
                    "label": "$option.TargetOperator.cases.ANY_6_STARS_OP.label",
                    "pipeline_override": {
                        "AutoHeadhunting": {
                            "attach": {
                                "TargetOperator": "ANY_6_STARS_OP"
                            }
                        }
                    }
                },
                {
                    "name": "Ardelia",
                    "label": "$operator.Ardelia",
                    "pipeline_override": {
                        "AutoHeadhunting": {
                            "attach": {
                                "TargetOperator": "Ardelia"
                            }
                        }
                    }
                },
