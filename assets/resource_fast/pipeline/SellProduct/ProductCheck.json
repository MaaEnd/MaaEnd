{
    "SellProductZeroProductColorMatch": {
        "doc": "识别红色文字判断是否有货",
        "recognition": {
            "type": "ColorMatch",
            "param": {
                "roi": [
                    1083,
                    327,
                    109,
                    24
                ],
                "method": 40,
                "lower": [
                    0,
                    30,
                    80
                ],
                "upper": [
                    5,
                    150,
                    200
                ],
                "count": 10
            }
        }
    },
    "SellProductWhiteTextPosition": {
        "doc": "识别白色文字位置，判断是否有货",
        "recognition": {
            "type": "ColorMatch",
            "param": {
                "roi": [
                    1083,
                    327,
                    109,
                    24
                ],
                "method": 4,
                "lower": [
                    190,
                    190,
                    190
                ],
                "upper": [
                    255,
                    255,
                    255
                ],
                "count": 10
            }
        }
    },

    "SellProductZeroProduct": {
        "doc": "货物数量不足，先尝试更换货品，失败则换据点",
        "recognition": {
            "type": "Or",
            "param": {
                "any_of": [
                    "SellProductZeroProductColorMatch",
                    {
                        "recognition": {
                            "type": "And",
                            "param": {
                                "all_of": [
                                    "SellProductWhiteTextPosition",
                                    {
                                        "recognition": {
                                            "type": "OCR",
                                            "param": {
                                                "roi": "SellProductWhiteTextPosition",
                                                "expected": [
                                                    "^[0-9]{1,3}$"
                                                ],
                                                "only_rec": true
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        },
        "next": [
            "SellProductChangeProduct",
            "SellProductOutpostDone"
        ]
    },
    "SellProductChangeProduct": {
        "doc": "识别更换货品并点击，进入更换UI",
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": [
                    // 更换货品
                    "更换货品",
                    "更換貨品",
                    "Switch Goods",
                    "商品変更",
                    "상품 교체"
                ],
                "roi": [
                    1115,
                    344,
                    172,
                    149
                ]
            }
        },
        "action": {
            "type": "Click",
            "param": {}
        },
        "next": [
            "SellProductSelectNewProduct"
        ]
    },
    "SellProductSelectNewProduct": {
        "doc": "在更换UI中识别新货物并点击",
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": [
                    // 本地仓储
                    "本地仓储",
                    "本地區倉儲",
                    "Local Depot",
                    "地域在庫",
                    "지역 저장고"
                ],
                "roi": [
                    511,
                    180,
                    274,
                    83
                ]
            }
        },
        "action": {
            "type": "Click",
            "param": {}
        },
        "next": [
            "SellProductSelectNewProductConfirm",
            "SellProductSelectNewProduct"
        ]
    },
    "SellProductSelectNewProductConfirm": {
        "doc": "点击确认",
        "recognition": {
            "type": "And",
            "param": {
                "all_of": [
                    "WhiteConfirmButtonType2"
                ]
            }
        },
        "action": {
            "type": "Click",
            "param": {}
        },
        "next": [
            "SellProductCanSellAfterChange"
        ]
    },
    "SellProductCanSellAfterChange": {
        "doc": "更换货品后重新判断能否售卖",
        "next": [
            "SellProductZeroProductRetry",
            "SellProductZeroMoney",
            "SellProductClickBarRight",
            "SellProductDragBar"
        ]
    },
    "SellProductZeroProductRetry": {
        "doc": "更换货品后仍无货，进入换据点流程",
        "focus": {
            "Node.Action.Succeeded": "当前选择产品缺货，请制造或更换"
        },
        "recognition": "And",
        "all_of": [
            "SellProductZeroProduct"
        ],
        "next": [
            "SellProductOutpostDone"
        ]
    },
    "SellProductZeroMoney": {
        "focus": {
            "Node.Action.Succeeded": "该据点调度券不足，过一会再来吧"
        },
        "doc": "调度券余量为0",
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": [
                    // 不足
                    "不足",
                    "不足",
                    "Insufficient",
                    "insufficient",
                    "不足",
                    "거점 관리권 보유량 부족"
                ],
                "roi": [
                    900,
                    611,
                    350,
                    77
                ]
            }
        },
        "next": [
            "SellProductOutpostDone"
        ]
    }
}
